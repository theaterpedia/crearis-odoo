<?xml version="1.0"?>
<odoo>
    <record id="view_version_form" model="ir.ui.view">
        <field name="name">Version Form</field>
        <field name="model">crearis.version</field>
        <field name="arch" type="xml">
            <form string="Domain User">
                <group>
                    <field name="write_date" />
                    <field name="cid" />
                    <field name="version" />
                    <field name="name" />
                    <field name="vtype" />
                    <field name="domain_ids" />
                    <field name="author_id" />                  
                    <field name="note" />
                </group>
            </form>         
        </field>
    </record>
    <record id="view_tree_version" model="ir.ui.view">
        <field name="name">Version List</field>
        <field name="model">crearis.version</field>
        <field name="arch" type="xml">
            <tree>
                <field name="write_date" />
                <field name="cid" />
                <field name="version" />                
                <field name="name" />
                <field name="domain_ids" />
                <field name="vtype" />
                <field name="name" />
                <field name="author_id" />
                <field name="note" />
            </tree>
        </field>
    </record>
    <record id="view_search_version" model="ir.ui.view">
        <field name="name">Version Filters</field>
        <field name="model">crearis.version</field>
        <field name="arch" type="xml">
            <search>

            </search>
        </field>
    </record>          
</odoo>